  "scripts": {
    "preinstall": "npm prune",
    "postinstall": "selenium-standalone install",
    "prebuild": "npm run -s clean",
    "build": "webpack -p",
    "build:visualize": "npm run build -- --env.visualize",
    "build:debug": "npm run build -- --progress --profile --colors --display-modules --display-reasons --display-error-details",
    "test": "karma start --single-run",
    "tdd": "karma start --auto-watch",
