    "resetmockdb": "curl -s -o /dev/null -w 'Status code: %{http_code}' -XPOST http://localhost:8070/resetmockdb",
    "resetmockdb:hard": "curl -s -o /dev/null -w 'Status code: %{http_code}' -XPOST http://localhost:8070/resetmockdbhard"
